.pagebox {
  position: relative;
  overflow: hidden;
  background-color: #f6f8f8;
}

.listbox1 {
  position: relative;
  height: 480rpx;
  width: 350rpx;
  background-color: #fff;
  margin-bottom: 30rpx;
  margin-left: 20rpx;
  float: left;
  box-shadow: 0 10rpx 20rpx rgba(0, 0, 0, 0.1);
}

.listbox1 .bgimg {
  width: 95%;
  padding-top: 10rpx;
  height: 400rpx;
}

.listbox1 .avatar {
  position: absolute;
  bottom: 10rpx;
  left: 30rpx;
  width: 50rpx;
  height: 50rpx;
  border: 4rpx solid #fff;
  border-radius: 100%;
}

.listbox1 .nickname {
  position: absolute;
  bottom: 10rpx;
  left: 110rpx;
  font-size: 26rpx;
  color: #9a9a9a;
  max-width: 40%;
  height: 60rpx;
  line-height: 60rpx;
  overflow: hidden;
}

.listbox1 .title {
  display: block;
  font-size: 32rpx;
  padding-left: 20rpx;
  color: #0e0e0e;
  width: 330rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  margin-top: 3px;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.listbox1 .tj {
  position: absolute;
  display: flex;
  text-align: right;
  height: 50rpx;
  bottom: 5rpx;
  right: 10rpx;
  line-height: 50rpx;
}

.listbox1 .tjimg {
  width: 40rpx;
  height: 40rpx;
  margin-right: 5rpx;
}

.listbox1 .tjsum {
  font-size: 25rpx;
  color: #9a9a9a;
  max-width: 100rpx;
  height: 60rpx;
  line-height: 40rpx;
  text-align: left;
  margin-right: 10rpx;
}

