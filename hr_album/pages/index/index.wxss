    /* margin-bottom: 30rpx; */
}

.code {
    margin: 0;
    padding: 0;
    border: 0;
    width: 600px;
    height: 900px;
}

.gb {
    position: fixed;
    top: 99999rpx;
    left: 99999rpx;
    width: 700px;
    height: 1050rpx;
}

button {
    content: "";
    border: 0px;
    padding: 0;
    margin: 0;
    line-height: 20px;
    background: none;
    overflow: visible;
    box-sizing: inherit;
    border-radius: 0;
}

button::after {
    border: none;
}

.kfbtn {
    position: fixed;
    bottom: 50rpx;
    width: 100rpx;
    height: 100rpx;
    right: 10rpx;
}

.kfimg {
    width: 100%;
    height: 100%;
}














.u_list {
  margin-top:-10px;
    display: flex;
    top: 150rpx;
    height: 70rpx;
    color: #000;
    text-align: center;
    width:98%;
background-color:white;
margin:0 auto;
}

.u_icos {
    width: 50rpx;
    height: 50rpx;
    margin-right: 5rpx;
}

.u_list .u_items {
    display: flex;
    min-width: 350rpx;
