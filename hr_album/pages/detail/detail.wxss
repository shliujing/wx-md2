.main {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
  position: absolute;
}


.addbox {
    background-color: white;
    height: 200rpx;
    margin-top: 30rpx;
    border-radius: 10rpx;
    border: 1px solid #E9EAEF;
    display: flex;
    align-items: center;
    flex-direction: column;
}

.iocbox {
    width: 40rpx;
    height: 40rpx;
    background-color: #FD5A87;
    border-radius: 100%;
    padding: 30rpx;
    margin: 30rpx 30rpx 15rpx 30rpx;
}

.descss {
    font-size: 28rpx;
    letter-spacing: 5rpx;
}

.imgs {
    width: 100%;
    height: 100%;
}

.baby-avatar {
  /* top: -70rpx; */
